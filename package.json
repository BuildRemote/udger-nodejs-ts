{
  "name": "@buildremote/udger-nodejs-ts",
  "version": "1.6.3",
  "type": "module",
  "description": "NodeJS User-Agent String Parser based on Udger SQLite databases https://udger.com/products/local_parser",
  "exports": {
    ".": "./dist/index.js"
  },
  "module": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "directories": {
    "test": "test"
  },
  "tap": {
    "coverage": false,
    "node-arg": [
      "--no-warnings",
      "--loader",
      "ts-node/esm"
    ],
    "ts": false
  },
  "scripts": {
    "build": "rm -r dist/ && tsc && cp defaultResult.json dist/",
    "prepublish": "yarn build",
    "test": "c8 tap"
  },
  "repository": {
    "url": "https://github.com/udger/udger-nodejs.git"
  },
  "keywords": [
    "udger",
    "user-agent",
    "device-detector",
    "bot-detection",
    "mobile-detection",
    "bot",
    "bots",
    "antibot",
    "antibots",
    "ipv4",
    "ipv6",
    "datacenter",
    "crawler"
  ],
  "author": "The Udger.com Team",
  "email": "info@udger.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/udger/udger-nodejs/issues"
  },
  "homepage": "https://github.com/udger/udger-nodejs#readme",
  "devDependencies": {
    "@types/tap": "^15.0.8",
    "c8": "^8.0.1",
    "connect": "3.7.0",
    "merge-deep": "3.0.3",
    "tap": "^16.3.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@laverdet/beaugunderson-ip-address": "^8.1.0",
    "@types/better-sqlite3": "^7.6.4",
    "better-sqlite3": "8.5.1",
    "dot-prop": "8.0.2",
    "fs-extra": "11.1.1",
    "node-gyp": "9.4.0",
    "randexp": "0.5.3",
    "zod": "^3.22.1"
  }
}
